{{# page_config }}
  <div>
    <div class="campaign-nav">
      <ul class="clearfix">
      {{# sub_nav_items }}
        {{> shared/menu_item }}
      {{/ sub_nav_items }}
      </ul>
    </div>
  </div>

  {{# active_sub_nav_item }}
    {{# has_sub_sub_nav_items? }}
      <div>
        <div class="campaign-sub-nav">
          <ul class="clearfix">
            {{# sub_sub_nav_items }}
              {{> shared/menu_item }}
            {{/ sub_sub_nav_items }}
          </ul>
        </div>
      </div>
    {{/ has_sub_sub_nav_items? }}
  {{/ active_sub_nav_item }}
{{/ page_config }}
